{
  "name": "gravity",
  "productName": "Gravity",
  "version": "0.0.1",
  "description": "a modern soundboard app for dank drops",
  "license": "MIT",
  "repository": "RiskyMcdanger/gravity",
  "author": {
    "name": "Risky McDanger",
    "email": "riskymcdanger@gmail.com",
    "url": "null"
  },
  "scripts": {
    "start": "electron .",
    "win32": "electron-packager . --out=dist --asar --overwrite --platform=win32 --arch=ia32",
    "win64": "electron-packager . --out=dist --asar --overwrite --platform=win32 --arch=x64",
    "linux32": "electron-packager . --out=dist --asar --overwrite --platform=linux --arch=ia32",
    "linux64": "electron-packager . --out=dist --asar --overwrite --platform=linux --arch=x64",
    "osx": "electron-packager . --out=dist --asar --overwrite --platform=darwin --arch=x64",
    "build": "electron-packager . --out=dist --asar --overwrite --all"
  },
  "main": "main.js",
  "files": [
    "main.js",
    "index.js",
    "index.html",
    "index.css"
  ],
  "keywords": [
    "audio",
    "electron",
    "soundboard"
  ],
  "dependencies": {
    "drag-drop": "^2.13.2",
    "electron": "^1.8.4",
    "electron-config": "^1.0.0",
    "electron-updater": "^2.21.4",
    "fs": "^0.0.1-security",
    "materialize-css": "^1.0.0-beta",
    "sortablejs": "^1.7.0",
    "uuid": "^3.2.1",
    "webpack": "^4.4.1"
  },
  "devDependencies": {
    "electron-packager": "^11.2.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.10.0"
  }
}
